{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.css$": "<rootDir>/emptyModule.js"
  },
  "transform": {
    "\\.svg$": "<rootDir>/fileTransformer.js"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/setup-jest.ts"
  ],
  "coveragePathIgnorePatterns": [
    "App.tsx",
    "main.tsx",
    "header",
    "home/index.tsx"
  ]
}