{
  "name": "vuttr-vite",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "author": "Caique Giovannini",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server\"",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node-esm' ./server/index.ts",
    "test": "jest",
    "test-cover": "jest --collectCoverage --collectCoverageFrom=\"./src/**/*.{ts,tsx}\" --coverageDirectory=\"coverage\"",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@supabase/supabase-js": "^2.42.1",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "json-server": "^0.17.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "axios-mock-adapter": "^1.21.5",
    "concurrently": "^8.2.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
